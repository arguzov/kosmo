<%
   content = @service.content
   if @service.id.to_i != 79
       content = content.gsub(/<img[^>]*src=([^ ]*)[^>]*>/i,'')
       content = content.gsub(/<iframe[^>]*src=([^ >]*)[^>]*>/i,'')
       content = content.gsub(/<h2[^>]*>([^<]*)<\/h2>/i) {|s| s.gsub('<h2','<h2 id="' + Digest::MD5.hexdigest(s) + '"')}
   end
%>
<%=content.html_safe%>