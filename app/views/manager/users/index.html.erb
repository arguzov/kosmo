<div class="wrapper wrapper-content animated fadeInRight">
  <div class="ibox">
    <div class="ibox-title">
      Список клиентов
      <div class="pull-right"><a href="/manager/users/new" class="btn btn-success btn-sm">Новый клиент</a></div>
    </div>
    <div class="ibox-content">
      <input type="hidden" id="order-id" value="0">
      <table class="table" id="manager-orders">
        <tr>
          <th>Id</th>
          <th>Имя</th>
          <th>Телефон</th>
          <th>Email</th>
          <th>Редактировать</th>
          <th>Удалить</th>
        </tr>
        <%@users.each do |user|%>
            <tr>
              <td><%=user.id%></td>
              <td><%=user.name%></td>
              <td><%=user.phone%></td>
              <td><%=user.email%></td>
              <td><a href="/manager/users/<%=user.id%>/edit" class="btn btn-primary">Редактировать</a></td>
              <td><%= link_to "Удалить", manager_user_path(user), method: :delete, class: "btn btn-danger" %></td>
            </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>